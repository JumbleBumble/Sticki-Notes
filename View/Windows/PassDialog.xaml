<Window x:Class="StickyNotesJ.View.Windows.PassDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StickyNotesJ.View.Windows"
        xmlns:userControl="clr-namespace:StickyNotesJ.userControl"
        mc:Ignorable="d"
        Title="Password" Height="116" Width="234" ResizeMode="CanMinimize" Icon="/View/Windows/Icon.png">
    
    <Grid  Background="#315682">
        <Border Margin="0,15,0,0" CornerRadius="3" Background="#222E3B" Width="120" Height="20" VerticalAlignment="Top">
            <userControl:JTextbox x:Name="PasswordInput" Placeholder="Password" OriginalText=""/>
        </Border>
        <Border Width="60" Height="25" CornerRadius="3" Background="#122E3B" Margin="0,0,0,10" VerticalAlignment="Bottom">
            <Button x:Name="EnterPassBtn" Foreground="White" BorderBrush="Transparent" Background="Transparent" Content="Enter" Click="EnterPassBtn_Click"/>
        </Border>
    </Grid>
</Window>
